more "$(dirname "$0")/EULA.txt"

echo ""
echo ""
echo "(Note: you can also use \"-e ACCEPT_EULA=Y\" to indicate agreement.)"
echo ""
read -ep "Do you agree to the EULA? " R
if [[ "${R,,}" != @("y"|"yes") ]]; then echo "Bye."
else ACCEPT_EULA=Y launcher; fi
