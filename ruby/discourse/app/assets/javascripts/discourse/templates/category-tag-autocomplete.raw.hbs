<div class='autocomplete ac-category-or-tag'>
  <ul>
    {{#each options as |option|}}
      <li>
        {{#if option.model}}
          <a href>{{category-link option.model allowUncategorized="true" link="false"}}</a>
        {{else}}
          <a href>{{d-icon 'tag'}}{{option.name}} x {{option.count}}</a>
        {{/if}}
      </li>
    {{/each}}
  </ul>
</div>
