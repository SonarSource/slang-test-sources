fun <T> doSomething(a: T) {}

fun test(n: Int) {
    val res: String

    when (n) {
        1 -> {
            res = "one"
            val x = "A"
            doSomething(x)
            val y = "AA"
            doSomething(y)
        }
        2 -> {
            res = "two"
            val x = "B"
            doSomething(x)
            val y = "BB"
            doSomething(y)
        }
        else -> {
            res = "unknown"
            val x = "C"
            doSomething(x)
            val y = "CC"
            doSomething(y)
        }
    }
}
