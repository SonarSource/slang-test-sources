fun <T> doSomething(a: T) {}

fun test(n: Int) {
    val res: String

    <caret>when {
        n == 1 -> {
            res = "one"
            doSomething("A")
        }
        n == 2 -> {
            res = "two"
            doSomething("B")
        }
        else -> {
            res = "unknown"
            doSomething("C")
        }
    }
}
